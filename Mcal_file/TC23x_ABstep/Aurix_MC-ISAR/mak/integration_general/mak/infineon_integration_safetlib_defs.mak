#*****************************INTEGRATION specific conditions**************

SAFETLIB_INTEGRATION="-DSAFETLIB_USED=STD_OFF"
ifeq ($(SAFETLIB_USED),STD_ON)
SAFETLIB_INTEGRATION="-DSAFETLIB_USED=STD_ON"
endif

STL_TARGET_EP=FALSE

ifeq ($(STL_TARGET),23)
STL_TARGET_EP=TRUE
endif

ifeq ($(STL_TARGET),22)
STL_TARGET_EP=TRUE
endif

ifeq ($(STL_TARGET),21)
STL_TARGET_EP=TRUE
endif

SAFET_BASE_FOLDER = ../../../01_SafeTlib
SAFET_APP_PROJSRC = $(SAFET_BASE_FOLDER)/Application/src
SAFET_COMMON_PROJSRC = $(SAFET_BASE_FOLDER)/Common
SAFET_UPPER_PROJSRC = $(SAFET_BASE_FOLDER)/UpperLayer
SAFET_SBST_FOLDER = $(SAFET_BASE_FOLDER)/SBST

SAFETLIB_OBJECTS_LINK_ONLY=
ifeq ($(SAFETLIB_USED),STD_ON)
#***************SafeTLib Demo***************************
### Safatlib Temporary directory to link assembly files
ifeq ($(COMPILER_CHOICE),GNU)
SAFET_TEMP_OUTDIR = $(SAFET_BASE_FOLDER)/Application/debug/TC$(STL_TARGET)x/gnu
endif

ifeq ($(COMPILER_CHOICE),WINDRIVER)
SAFET_TEMP_OUTDIR = $(SAFET_BASE_FOLDER)/Application/debug/TC$(STL_TARGET)x/windriver
endif

ifeq ($(COMPILER_CHOICE_TP),tasking)
SAFET_TEMP_OUTDIR = $(SAFET_BASE_FOLDER)/Application/debug/TC$(STL_TARGET)x/tasking
endif

ifeq ($(COMPILER_CHOICE_TP),tasking50r2)
SAFET_TEMP_OUTDIR = $(SAFET_BASE_FOLDER)/Application/debug/TC$(STL_TARGET)x/tasking50r2
endif

SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/RefApp.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/TstM.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/SafeWdgM.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/Sl_Ipc.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/AppCbk.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/SmuIntHandler.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/Sl_FlsErrPtrn.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/PmuEccEdcTst_PtrnDef.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_Kernel_CoreTest.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_Kernel_ISG.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_Kernel_TestCode.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_TC16E_TestCode.$(OBJ_FILE_SUFFIX)
ifeq ($(STL_TARGET_EP),FALSE)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_TC16P_CoreTest.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_TC16P_ISG.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_TC16P_TestCode.$(OBJ_FILE_SUFFIX)
endif
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_SBST_FOLDER)/SBST_RelocTable.$(OBJ_FILE_SUFFIX)
SAFETLIB_OBJECTS_LINK_ONLY += $(SAFET_TEMP_OUTDIR)/SafeTlibCD.a $(SAFET_TEMP_OUTDIR)/SafeWdgCD.a

endif
