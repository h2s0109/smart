<datamodel version="3.0"
           xmlns="http://www.tresos.de/_projects/DataModel2/08/root.xsd"
           xmlns:a="http://www.tresos.de/_projects/DataModel2/08/attribute.xsd"
           xmlns:v="http://www.tresos.de/_projects/DataModel2/06/schema.xsd"
           xmlns:d="http://www.tresos.de/_projects/DataModel2/06/data.xsd">

  <d:ctr type="AUTOSAR" factory="autosar"
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd"
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd"
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd">
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="AURIX" type="AR-PACKAGE">
        <a:a name="UUID" value="e656b9a5-1655-40db-ad91-29dd3b34035e"/>
        <d:lst type="ELEMENTS">
          <d:chc name="Port" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                <ad:ADMIN-DATA>
                  <ad:DOC-REVISIONS>
                    <ad:DOC-REVISION>
                      <ad:REVISION-LABEL>3.2.0</ad:REVISION-LABEL>
                      <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                      <ad:DATE>2010-11-03</ad:DATE>
                    </ad:DOC-REVISION>
                  </ad:DOC-REVISIONS>
                </ad:ADMIN-DATA>
              </a:a>
              <a:a name="RELEASE" value="asc:4.0"/>
              <a:a name="UUID" value="77b88fa1-a26a-47b1-842b-48059e84f41e"/>
              <d:ref type="REFINED_MODULE_DEF" value="ASPath:/AUTOSAR/EcucDefs/Port"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
			  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
				</a:a>  
                <a:a name="LABEL" value="ConfigVariant"/>
                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                <a:da name="RANGE">
                  <a:v>VariantPostBuildLoadable</a:v>
                  <a:v>VariantPostBuildSelectable</a:v>
                </a:da>
                <a:da name="EDITABLE" value="true"/>
              </v:var>
              <v:lst name="PortConfigSet"
                     type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MIN" value="1"/>
                <v:ctr name="PortConfigSet"
                       type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC"
                       value="EN: This container is the base of a multiple configuration set"/>
                  <a:a name="UUID" value="2b7f9475-2cf3-4de7-a0dd-acdeac7c902e"/>
                  <v:lst name="PortContainer" type="MAP">
                    <a:da name="MIN" type="XPath" expr="ecu:get('Port.TotalNumOfPort')"/>
                    <a:da name="MAX" type="XPath" expr="ecu:get('Port.TotalNumOfPort')"/>
                    <v:ctr name="PortContainer" type="IDENTIFIABLE">
                      <a:a name="DESC"
                           value="EN: Container collecting the PortPins."/>
                      <a:a name="UUID"
                           value="58c0d0d6-418a-437b-b59a-d4cba3e4d1f2"/>
                      <v:var name="PortNumber" type="INTEGER">
                        <a:da name="TOOLTIP" value="The number of specified Port in this PortContainer."/>
                        <a:a name="DESC"
                             value="EN: The number of specified Port in this PortContainer."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID"
                             value="2b452b26-b125-4997-8a7f-b50e9d257ca4"/>
                        <a:da name="EDITABLE" value="false"/>
                        <a:da name="DEFAULT" type="XPath">
                        <a:tst expr="text:split(ecu:get('Port.AvailablePorts'), ' ')[position()-1 = node:fallback(node:current()/../@index,'0')]"/>
                        </a:da>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath expr="contains(ecu:get('Port.PortsAvailable'),concat('_',.,'_'))" false="The Specified PortNumber is not available in the MC"/>
                          <mt:range>
                            <mt:tst expr="&gt;=0"/>
                            <mt:tst expr="&lt;=ecu:get('Port.MaxAvailablePort')"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:var name="PortNumberOfPortPins" type="INTEGER">
                        <a:da name="TOOLTIP" value="The number of PortPins available in this Port."/>
                        <a:a name="DESC"
                             value="EN: The number of PortPins available in this Port."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID"
                             value="2b452b26-b1b5-4997-8a7f-b50e9d257ca4"/>
                        <a:da name="EDITABLE" value="false"/>
                        <a:da name="DEFAULT" type="XPath">
                          <a:tst expr="text:split(ecu:get('Port.PortNoOfpins'), ' ')[position()-1 = node:fallback(node:current()/../@index,'0')]"/>
                        </a:da>
                        <a:da name="INVALID" type="Multi">
                          <mt:xpath expr="num:i(count(text:split(ecu:get(concat('Port.Port',../PortNumber,'_AvailablePins')),' '))) = num:i(.)" false="Incorrect number of pins"/>
                          <mt:range>
                            <mt:tst expr="&gt;=1"/>
                            <mt:tst expr="&lt;=16"/>
                          </mt:range>
                        </a:da>
                      </v:var>
                      <v:lst name="PortPin" type="MAP">
                       <a:da name="MIN" type="XPath" expr="node:fallback(node:current()/../PortNumberOfPortPins,'1')"/>
                       <a:da name="MAX" type="XPath" expr="node:fallback(node:current()/../PortNumberOfPortPins,'16')"/>
                        <v:ctr name="PortPin" type="IDENTIFIABLE">
                          <a:a name="DESC">
                            <a:v>EN: Configuration of the individual port pins.
                            </a:v>
                          </a:a>
                          <a:a name="UUID"
                               value="805d3334-920b-4f9b-b18f-ce2e8a6c2f9f"/>
                          <v:var name="PortPinId" type="INTEGER">
                            <a:da name="TOOLTIP" value="Pin Id of the port pin. This value will be assigned to the symbolic name derived from the port pin container short name."/>
                            <a:a name="DESC"
                                 value="EN: Pin Id of the port pin. This value will be assigned to the symbolic name derived from the port pin container short name."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                 type="IMPLEMENTATIONCONFIGCLASS">
                              <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                              <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="630cfba7-0e08-47ab-a9af-b8441e19b081"/>
                            <a:da name="EDITABLE" value="false"/>
                            <a:da name="DEFAULT" type="XPath">
                            <a:tst expr="text:split(ecu:get(concat('Port.PinId_Port',(node:fallback(node:current()/../../../PortNumber,'0')))), ' ')[position()-1 = node:fallback(node:current()/../@index,'0')]"/>
                            </a:da>
                            <a:da name="INVALID" type="Multi">
                              <mt:xpath expr="contains(ecu:get(concat('Port.PinId_Port',../../../PortNumber)),.)" false="Incorrect PortPinID"/>
                              <mt:range>
                                <mt:tst expr="&gt;=0"/>
                                <mt:tst expr="&lt;=672"/>
                              </mt:range>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinSymbolicName" type="STRING">
                            <a:da name="TOOLTIP" value="Symbolic name of the Port Pin."/>
                            <a:a name="DESC"
                                 value="EN: Symbolic name of the Port Pin."/>
                            <a:a name="UUID"
                                 value="c7902328-d273-4512-9bad-0e756751ad73"/>
                                 <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:da name="EDITABLE" value="true"/>
                            <a:da name="DEFAULT" type="XPath">
                               <a:tst expr="concat('PORT_',(node:fallback(node:current()/../../../PortNumber,'0')),'_PIN_',((node:fallback(node:current()/../PortPinId,'0')) mod 16))"/>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                               <a:tst expr="text:grep(., '^[A-Z][a-zA-Z0-9_]*$')" false="Must start with a capital letter. Special characters other than '_' is not supported"/>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinDirection" type="ENUMERATION">
                           <a:da name="TOOLTIP" value="The initial direction of the pin (IN or OUT). If the direction is not changeable, the value configured here is fixed.
The direction must match the pin mode. E.g. a pin used for an ADC must be configured to be an in port.
Implementation Type: Port_PinDirectionType">
                             </a:da>
                            <a:a name="DESC">
                              <a:v>EN: The initial direction of the pin (IN or OUT). If the direction is not changeable, the value configured here is fixed.
The direction must match the pin mode. E.g. a pin used for an ADC must be configured to be an in port.
Implementation Type: Port_PinDirectionType
                            </a:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="53170d11-cda1-4f50-8754-4453e1904683"/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="DEFAULT" value="PORT_PIN_IN"/>
                            <a:da name="RANGE">
                              <a:v>PORT_PIN_IN</a:v>
                              <a:v>PORT_PIN_OUT</a:v>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                             <a:tst expr="((../../../PortNumber = '40') or (../../../PortNumber = '41'))and (.='PORT_PIN_OUT')" true="Invalid configuration, Port 40 and 41 only PORT_PIN_IN configuration is allowed. "/>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinDirectionChangeable" type="BOOLEAN">
                            <a:da name="TOOLTIP" value="Parameter to indicate if the direction is changeable on a port pin during runtime.
true: Port Pin direction changeable enabled.
false: Port Pin direction changeable disabled.">
                             </a:da>
                            <a:a name="DESC">
                              <a:v>EN: Parameter to indicate if the direction is changeable on a port pin during runtime.
true: Port Pin direction changeable enabled.
false: Port Pin direction changeable disabled.
                            </a:v>
                            </a:a>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="48dcc5c1-f7c6-417f-a2b1-e8cd402dcc28"/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="DEFAULT" value="false"/>
                            <a:da name="EDITABLE" type="XPath">
                            <a:tst expr= "(../../../../../../../PortGeneral/PortSetPinDirectionApi = 'true') and ((../../../PortNumber != '40') and (../../../PortNumber != '41'))"/>
                            </a:da>
                            <a:da name="INVALID" type="XPath">
                            <a:tst expr= "(../../../../../../../PortGeneral/PortSetPinDirectionApi = 'false') and (.='true')" true="Invalid configuration. PortSetPinDirectionApi needs to be enabled"/>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinInputCharacteristic" type="ENUMERATION">
                            <a:da name="TOOLTIP" value="Configuration of input characteristics (electrical) for input pins.
If the pin direction is initialized as input, this configuration parameter is taken during initialization (by Port_Init API).">
                            </a:da>
                            <a:a name="DESC">
                             <a:v>Configuration of input characteristics (electrical) for input pins.
If the pin direction is initialized as input, this configuration parameter is taken during initialization (by Port_Init API). The user must configure this.
If the pin direction is initialized as output, this configuration parameter is ignored during initialization.
If direction changeable is set to true, this parameter is used for configurating of pin to input during runtime.
PORT_PIN_IN_NO_PULL: No input pull device connected
PORT_PIN_IN_PULL_DOWN: Input pull-down device connected
PORT_PIN_IN_PULL_UP: Input pull-up device connected
                             </a:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="70907698-b2f6-49b0-bd1a-456c1b30de95"/>
                                 <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="ENABLE" type="XPath">
                            <a:tst expr= "((../PortPinDirection = 'PORT_PIN_IN') or (../PortPinDirectionChangeable = 'true'))" />
                            </a:da>
                            <a:da name="DEFAULT" value="PORT_PIN_IN_PULL_UP"/>
                            <a:da name="RANGE">
                              <a:v>PORT_PIN_IN_NO_PULL</a:v>
                              <a:v>PORT_PIN_IN_PULL_DOWN</a:v>
                              <a:v>PORT_PIN_IN_PULL_UP</a:v>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinOutputCharacteristic" type="ENUMERATION">
                            <a:da name="TOOLTIP" value="Configuration of output characteristic (electrical) for the output pins.
If the pin direction is initialized as output, this configuration parameter is taken during initialization (by Port_Init API).">
                            </a:da>
                            <a:a name="DESC">
                             <a:v>Configuration of output characteristic (electrical) for the output pins.
If the pin direction is initialized as output, this configuration parameter is taken during initialization (by Port_Init API). The user must configure this.
If the pin direction is initialized as input, this configuration parameter is ignored during initialization.
If direction changeable is set to true, this parameter is used for configurating of pin to output during runtime.
PORT_PIN_OUT_PUSHPULL: Push-pull functionality for output pins
PORT_PIN_OUT_OPENDRAIN: Open-drain functionality for output pins
                             </a:v>
                            </a:a>
                            <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="ed12eb00-b2f7-434c-8ad0-0001a509a0af"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="ENABLE" type="XPath">
                            <a:tst expr= "((../PortPinDirection = 'PORT_PIN_OUT') or (../PortPinDirectionChangeable = 'true'))" />
                            </a:da>
                            <a:da name="DEFAULT" value="PORT_PIN_OUT_PUSHPULL"/>
                            <a:da name="RANGE">
                              <a:v>PORT_PIN_OUT_PUSHPULL</a:v>
                              <a:v>PORT_PIN_OUT_OPENDRAIN</a:v>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinLevelValue" type="ENUMERATION">
                            <a:da name="TOOLTIP" value="Port Pin Level value from Port pin list."/>
                            <a:a name="DESC"
                                 value="EN: Port Pin Level value from Port pin list."/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="63dbd4d6-5fe1-4eac-9c3f-e82e81000d1d"/>
                                 <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="DEFAULT" value="PORT_PIN_LEVEL_LOW"/>
                            <a:da name="RANGE">
                              <a:v>PORT_PIN_LEVEL_HIGH</a:v>
                              <a:v>PORT_PIN_LEVEL_LOW</a:v>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinInitialMode" type="ENUMERATION">
                            <a:a name="DESC"
                                 value="EN: Port pin mode from mode list to be used in Port_Init() function. Keep the mouse over the parametername to know the supported modes of the pin."/>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:a name="UUID"
                                 value="ac00d2a8-d0e3-4329-9639-90b918215f31"/>
                                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                             type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                            </a:a>
                            <a:da name="TOOLTIP" type="XPath" expr="ecu:get(concat('Port.InitialModes',(../../../PortNumber),'_',((../PortPinId) mod 16)))"/>
                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                            <a:da name="INVALID" type="XPath">
                            <a:tst expr="../PortPinDirection = 'PORT_PIN_IN' and (.!='PORT_PIN_MODE_GPIO')" true="Invalid configuration, If PortPinDirection is PortPinIn then PortPinInitialMode should be PORT_PIN_MODE_GPIO only . "/>
                            </a:da>
                            <a:da name="DEFAULT" value="PORT_PIN_MODE_GPIO"/>
                            <a:da name="EDITABLE" type="XPath" expr="(../../../PortNumber != '40') and (../../../PortNumber != '41')"/>
                            <a:da name="RANGE">
                              <a:v>PORT_PIN_MODE_GPIO</a:v>
                              <a:v>PORT_PIN_MODE_ALT1</a:v>
                              <a:v>PORT_PIN_MODE_ALT2</a:v>
                              <a:v>PORT_PIN_MODE_ALT3</a:v>
                              <a:v>PORT_PIN_MODE_ALT4</a:v>
                              <a:v>PORT_PIN_MODE_ALT5</a:v>
                              <a:v>PORT_PIN_MODE_ALT6</a:v>
                              <a:v>PORT_PIN_MODE_ALT7</a:v>
                            </a:da>
                          </v:var>
                          <v:var name="PortPinModeChangeable" type="BOOLEAN">
