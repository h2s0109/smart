                                 type="INTEGER" value="14">
                          </d:var>
                </d:ctr>
                <d:ctr name="MCU_STBYR_RESET" type="IDENTIFIABLE">
                          <d:var name="McuResetReason"
                                 type="INTEGER" value="15">
                          </d:var>
                </d:ctr>
                <d:ctr name="MCU_RESET_MULTIPLE" type="IDENTIFIABLE">
