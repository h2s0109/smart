              <d:ref type="REFINED_MODULE_DEF" value="ASPath:/AUTOSAR/EcucDefs/Pwm"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                  <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                <a:da name="RANGE">
                  <a:v>VariantPostBuildLoadable</a:v>
                  <a:v>VariantPostBuildSelectable</a:v>
                </a:da>
              </v:var>
              <v:lst name="PwmChannelConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MIN" value="1"/>
                <v:ctr name="PwmChannelConfigSet" 
                       type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC" 
                       value="EN: Multiple Configuration Set Container"/>
                  <a:a name="UUID" value="f850d3fa-b5ae-4f1a-b9f9-5c406c058865"/>
                  <v:lst name="PwmChannel" type="MAP">
                    <a:da name="MIN" value="1"/>
                    <v:ctr name="PwmChannel" type="IDENTIFIABLE">
                      <a:a name="DESC" 
                           value="EN: Configuration of an individual PWM channel."/>
                      <a:a name="UUID" 
                           value="e4502f41-9748-4bb3-b141-3399bb30fa7e"/>
                      <v:lst name="PwmChannelClass">
                        <a:da name="MIN" value="1"/>
                        <a:da name="MAX" value="1"/>
                        <v:var name="PwmChannelClass" type="ENUMERATION">
                          <a:a name="DESC" 
                               value="EN: Class of PWM Channel.  ImplementationType: Pwm_17_Gtm_ChannelClassType."/>
                          <a:da name="TOOLTIP" value="Class of PWM Channel.  ImplementationType: Pwm_17_Gtm_ChannelClassType."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                          </a:a>
                          <a:a name="LONG-NAME">
                            <a:v>PWM_FIXED_PERIOD_CENTER_ALIGNED: PWM_FIXED_PERIOD_CENTER_ALIGNED</a:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="2d293219-e734-4c63-aa17-04c20a9acbc0"/>
                          <a:da name="DEFAULT" value="PWM_FIXED_PERIOD"/>
                          <a:da name="RANGE">
                            <a:v>PWM_FIXED_PERIOD</a:v>
                            <a:v>PWM_FIXED_PERIOD_SHIFTED</a:v>
                            <a:v>PWM_VARIABLE_PERIOD</a:v>
                            <a:v>PWM_FIXED_PERIOD_CENTER_ALIGNED</a:v>
                          </a:da>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="((. = 'PWM_VARIABLE_PERIOD') and (../../../../../../PwmConfigurationOfOptApiServices/PwmSetPeriodAndDuty = 'false'))" true="PWM_VARIABLE_PERIOD is not applicable if PwmSetPeriodAndDuty is set to false"/>
                          </a:da>
                        </v:var>
                    </v:lst>
                      <v:var name="PwmSafetySignal" type="ENUMERATION">
                        <a:a name="DESC" 
                             value="EN: Specifies the type of the Pwm channel"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="e53c91fc-14ed-438f-a51a-c43f685c2a58"/>
                        <a:da name="DEFAULT" value="PWM_QM_TYPE"/>
                        <a:da name="RANGE">
                          <a:v>PWM_QM_TYPE</a:v>
                          <a:v>PWM_ASIL_TYPE</a:v>
                        </a:da>
                        <a:da name="TOOLTIP" value="This parameter is supported only if PwmSafetyEnable is enabled. It specifies the type of the PWM channel"/>                          
                        <a:da name="EDITABLE" type="XPath">
                          <a:tst expr="../../../../../PwmSafety/PwmSafetyEnable  = 'true'"/>
                        </a:da>                        
                      </v:var>                                        
                      <v:var name="PwmChannelId" type="INTEGER">
                        <a:a name="DESC" 
                             value="EN: Channel Id of the PWM channel. This value will be assigned to the symbolic name derived of the PwmChannel container short name."/>
                        <a:da name="TOOLTIP" value="Channel Id of the PWM channel. This value will be assigned to the symbolic name derived of the PwmChannel container short name."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                          </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                        <a:a name="UUID" 
                             value="98652939-96ab-4a60-b7ce-67853999611d"/>
                        <a:da name="INVALID" type="Multi">
                         <mt:xpath expr="text:uniq(../../*/PwmChannelId,.)" false="PwmChannelId should be unique.Enter diff Number"/> 
                         <mt:xpath expr="node:isconsecutive(../../*/PwmChannelId,0)" false="not Consecutive"/>
                         <mt:range>
                          <mt:tst expr="&gt;=0"/>
                          <mt:tst expr="&lt;=4294967295"/>
                         </mt:range>
                        </a:da>
                         <a:da name="DEFAULT" type="XPath" expr="num:i((node:fallback((node:current()/../@index),0))+0)"/>
                        </v:var>
                      <v:var name="PwmShiftValue" type="INTEGER">
                        <a:a name="DESC">
                      <a:v>EN: Shift Value for PWM_FIXED_PERIOD_SHIFTED Signal.
Valid only when PwmChannelClass is PWM_FIXED_PERIOD_SHIFTED.
This depends on PwmDutyShiftInTicks parameter.
When the PwmDutyShiftInTicks is OFF, the Shift value should be entered as percentage:
0, represents 0% 
0x8000 represents 100%
When the PwmDutyShiftInTicks is ON, the Shift value should be entered as absolute ticks:
0, represents 0% 
Same value as Period value represents 100%</a:v>
                        </a:a>
                        <a:da name="TOOLTIP" value="Shift Value for PWM_FIXED_PERIOD_SHIFTED Signal."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="e4bab055-95ef-4fb0-b038-a015bdc1ca70"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Multi">
                         <mt:xpath expr="(node:fallback(../../../../../PwmGeneral/PwmDutyShiftInTicks,'false') = 'false') and (.&gt;32768)" true="Shift is in percentage; Maximum is 32768 (0x8000)"/> 
                        <mt:range>
                          <mt:tst expr="&lt;=65534"/>
                          <mt:tst expr="&gt;=0"/>
                        </mt:range>
                        </a:da>
                  <a:da name="ENABLE" type="XPath">
                  <a:tst expr="(../PwmChannelClass/*[num:i(1)] = 'PWM_FIXED_PERIOD_SHIFTED')"/>
                  </a:da>
                      </v:var>
                      <v:var name="PwmDutycycleDefault" type="INTEGER">
                        <a:a name="DESC">
                          <a:v>EN: Value of duty cycle used for Initialization
Depends on PwmDutyShiftInTicks parameter.
When the PwmDutyShiftInTicks is OFF, the Dutycyle should be entered as percentage:
0, represents 0% 
0x8000 represents 100%
When the PwmDutyShiftInTicks is ON, the Dutycyle should be entered as absolute ticks:
0, represents 0% 
Same value as Period value represents 100%</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:da name="TOOLTIP" value="Value of duty cycle used for Initialization."/>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="297a0729-7a7c-4267-892a-631b7b659b60"/>
                        <a:da name="DEFAULT" value="0"/>
                       <a:da name="INVALID" type="Multi">
                         <mt:xpath expr="(node:fallback(../../../../../PwmGeneral/PwmDutyShiftInTicks,'false') = 'false') and (.&gt;32768)" true="Duty is in percentage; Maximum is 32768 (0x8000)"/>
                       <mt:range>
                       <mt:tst expr="&gt;=0"/>
                       <mt:tst expr="&lt;=65534"/>
                       </mt:range>
                        </a:da>
                      </v:var>
                      <v:var name="PwmIdleState" type="ENUMERATION">
                        <a:a name="DESC" 
                             value="EN: The parameter PWM_IDLE_STATE represents the output state of the PWM after the signal is stopped (e.g. call of Pwm_SetOutputToIdle)."/>
                        <a:da name="TOOLTIP" value="The parameter PWM_IDLE_STATE represents the output state."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="8e24a403-6cc7-4b4c-9ed5-891074a707ca"/>
                        <a:da name="DEFAULT" value="PWM_LOW"/>
                        <a:da name="RANGE">
                          <a:v>PWM_HIGH</a:v>
                          <a:v>PWM_LOW</a:v>
                        </a:da>
                      </v:var>
                      <v:lst name="PwmNotification">
                        <a:da name="MAX" value="1"/>
                        <v:var name="PwmNotification" type="FUNCTION-NAME">
                          <a:a name="DESC" 
                               value="EN: Definition of the Callback function."/>
                          <a:da name="TOOLTIP" value="Definition of the Callback function."/>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                            <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" 
                               value="ce992da9-950a-490f-8725-b839953c679c"/>
                          <a:da name="DEFAULT" value="NULL_PTR"/>
                          <a:da name="EDITABLE" type="XPath">
                           <a:tst expr="../../../../../../PwmGeneral/PwmNotificationSupported = 'true'"/>
                          </a:da>
                        </v:var>
                      </v:lst>
                      <v:var name="PwmPeriodDefault" type="FLOAT">
                        <a:a name="DESC">
                          <a:v>EN: Default Period of the channel. Input Value in absolute ticks.
This parameter does not impact the channels of class PWM_FIXED_PERIOD_SHIFTED and PWM_FIXED_PERIOD_CENTER_ALIGNED.</a:v>
                        </a:a>
                        <a:da name="TOOLTIP" value="Default Period of the channel."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="c6a84657-cf20-43cf-8e14-7cb104e0aa1c"/>
                        <a:da name="DEFAULT" value="0"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=65534"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                  <a:da name="ENABLE" type="XPath">
                  <a:tst expr="(../PwmChannelClass/*[num:i(1)] = 'PWM_FIXED_PERIOD' or ../PwmChannelClass/*[num:i(1)] = 'PWM_VARIABLE_PERIOD')"/>
                  </a:da>
                      </v:var>
                      <v:var name="PwmPolarity" type="ENUMERATION">
                        <a:a name="DESC">
                          <a:v>EN: Defines the starting polarity of each PWM channel.
                          </a:v>
                        </a:a>
                        <a:da name="TOOLTIP" value="Defines the starting polarity of each PWM channel."/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="acfafd24-d46c-4cf2-b4d8-ed006767cf15"/>
                        <a:da name="DEFAULT" value="PWM_LOW"/>
                        <a:da name="RANGE">
                          <a:v>PWM_HIGH</a:v>
                          <a:v>PWM_LOW</a:v>
                        </a:da>
                      </v:var>
                      <v:var name="PwmCoherentUpdate" type="BOOLEAN">
                        <a:a name="DESC">
                          <a:v>EN: Setting this parameter to true determines channel is coherent ot not.
                          </a:v>
                        </a:a>
                        <a:da name="TOOLTIP" value="Setting this parameter to true determines channel is coherent ot not"/>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" 
                             value="cc44a967-9eb2-45c0-a13b-1cbf48845d81"/>
                        <a:da name="DEFAULT" value="false"/>
                  <a:da name="ENABLE" type="XPath">
                  <a:tst expr="((../../../../../PwmGeneral/PwmChannelCoherentSelection = 'true') and (../PwmChannelClass/*[num:i(1)] = 'PWM_FIXED_PERIOD' or ../PwmChannelClass/*[num:i(1)] = 'PWM_VARIABLE_PERIOD'))"/>  
                  </a:da> 
                      </v:var>
                      <v:ref name="PwmAssignedHwUnit" type="CHOICE-REFERENCE">
                        <a:a name="DESC">
                        <a:v>EN: The required GTM channel.
                           Choose the GTM TOM channel which will be used by the PWM driver.</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID" 
                             value="6f437ed0-d2ae-4a8e-b431-f86d5d448728"/>
                        <a:da name="REF">
                          <a:v>ASPathDataOfSchema:/AURIX/Mcu/McuModuleConfiguration/GtmConfiguration/Tom/TomChannel</a:v>
                        </a:da>
                       <a:a name="INVALID" type="XPath" expr="node:refvalid(.)"  false="The configured node does not exist or may not be referenced."/>
                      </v:ref>
                      <v:ref name="PwmReferenceChannel" type="REFERENCE">
                        <a:a name="DESC">
                        <a:v>EN: Reference channel for PWM_FIXED_PERIOD_SHIFTED, and PWM_FIXED_PERIOD_CENTER_ALIGNED.
Reference should be a channel of PWM_FIXED_PERIOD Only.</a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                          <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                        <a:a name="UUID" 
                             value="974fcd0a-d294-4e4d-860f-7d1d17eabf16"/>
                        <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Pwm/PwmChannelConfigSet/PwmChannel"/>
                        <a:da name="ENABLE" type="XPath">
                          <a:tst expr="(../PwmChannelClass/*[num:i(1)] = 'PWM_FIXED_PERIOD_SHIFTED' or ../PwmChannelClass/*[num:i(1)] = 'PWM_FIXED_PERIOD_CENTER_ALIGNED')"/>  
                        </a:da> 
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="contains(., ../../../@name)" false="The Reference Channel must be mapped on the same Config Set channel"/>
                        </a:da>
                      </v:ref>
                  </v:ctr>
                </v:lst>
                <v:lst name="PwmSyncGroup" type="MAP">
                  <a:da name="MIN" value="0"/>
                  <v:ctr name="PwmSyncGroup" type="IDENTIFIABLE">
                    <a:a name="DESC" 
                         value="EN: PWM channel groups defined here."/>
                    <a:a name="UUID" 
                         value="e4513f21-9721-4bc3-b241-3499bb50fa7e"/>
                    <v:lst name="PwmGroupChannels">
                      <a:da name="MIN" value="0"/>
